{
  "name": "@sprucelabs/spruce-view-plugin",
  "publishConfig": {
    "access": "public"
  },
  "skill": {
    "namespace": "view",
    "blockedCommands": {
      "sync.events": "You need to use pull.event.contracts in this project."
    },
    "commandOverrides": {
      "pull.event.contracts": "--destination ./src/tests"
    },
    "upgradeIgnoreList": [
      "globby",
      "@sprucelabs/mercury-core-events"
    ]
  },
  "version": "7.0.321",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "!build/esm/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Spruce View! ðŸ‘¾",
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/sspruce-view-plugin",
  "bugs": {
    "url": "https://github.com/sprucelabsai/sspruce-view-plugin/issues"
  },
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "yarn build.tsc && tsc --project tsconfig.dist.json && yarn build.resolve-paths && yarn build.esm-postbuild",
    "build.esm-postbuild": "esm-postbuild --target build/esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.resolve-paths.esm": "TS_NODE_PROJECT=`pwd`/tsconfig.dist.json resolve-path-aliases --target build/esm --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint.local": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w",
    "lint.tsc": "tsc -p . --noEmit",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "update.dependencies": "yarn clean.dependencies && yarn"
  },
  "dependencies": {
    "@sprucelabs/error": "^5.0.316",
    "@sprucelabs/heartwood-view-controllers": "^51.6.32",
    "@sprucelabs/schema": "^25.4.62",
    "@sprucelabs/spruce-core-schemas": "^22.6.241",
    "@sprucelabs/spruce-event-plugin": "^17.0.2",
    "@sprucelabs/spruce-event-utils": "^17.1.256",
    "@sprucelabs/spruce-skill-booter": "^17.0.2",
    "@sprucelabs/spruce-skill-utils": "^22.1.45",
    "@sprucelabs/spruce-test-fixtures": "^17.0.2",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^1.0.296",
    "@sprucelabs/jest-json-reporter": "^6.0.264",
    "@sprucelabs/jest-sheets-reporter": "^1.2.267",
    "@sprucelabs/mercury-client": "^17.4.19",
    "@sprucelabs/mercury-types": "^26.1.979",
    "@sprucelabs/resolve-path-aliases": "^1.0.236",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/test": "^7.7.247",
    "@sprucelabs/test-utils": "^3.0.299",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.11",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "conventional-changelog-sprucelabs": "^1.1.2",
    "eslint": "^8.4.0",
    "eslint-config-spruce": "^10.10.16",
    "inquirer": "^8.2.0",
    "jest": "^27.4.3",
    "jest-circus": "^27.4.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "maxWorkers": 4,
    "testTimeout": 300000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/esm/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    },
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 843037832,
          "testMap": {
            "helpfulErrorIfVcIsBroken": "I155",
            "registersViewsOnBoot": "I162"
          }
        }
      ]
    ]
  }
}
