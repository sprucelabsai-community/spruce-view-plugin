{
  "name": "@sprucelabs/spruce-view-plugin",
  "publishConfig": {
    "access": "public"
  },
  "skill": {
    "namespace": "view",
    "blockedCommands": {
      "sync.events": "You need to use pull.event.contracts in this project!"
    },
    "commandOverrides": {
      "pull.event.contracts": "--destination ./src/tests"
    },
    "upgradeIgnoreList": [
      "globby",
      "@sprucelabs/mercury-core-events",
      "@types/node"
    ]
  },
  "version": "15.0.1",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "!build/esm/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Spruce View! ðŸ‘¾",
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/sspruce-view-plugin",
  "bugs": {
    "url": "https://github.com/sprucelabsai/sspruce-view-plugin/issues"
  },
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "tsc --project tsconfig.dist.json && yarn build.resolve-paths && mv build esm && yarn build.esm-postbuild && yarn build.tsc && yarn build.resolve-paths && mv esm build/ && yarn clean.dist",
    "build.esm-postbuild": "esm-postbuild --target esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "clean.dist": "rm -rf build/__tests__ build/esm/__tests__",
    "fix.lint.local": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w",
    "lint.tsc": "tsc -p . --noEmit",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "update.dependencies": "yarn clean.dependencies && yarn"
  },
  "dependencies": {
    "@sprucelabs/error": "^5.0.450",
    "@sprucelabs/heartwood-view-controllers": "^71.0.2",
    "@sprucelabs/schema": "^27.1.69",
    "@sprucelabs/spruce-core-schemas": "^25.0.110",
    "@sprucelabs/spruce-event-plugin": "^31.0.2",
    "@sprucelabs/spruce-event-utils": "^19.1.117",
    "@sprucelabs/spruce-skill-booter": "^31.0.2",
    "@sprucelabs/spruce-skill-utils": "^25.1.4",
    "@sprucelabs/spruce-test-fixtures": "^31.0.2",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^1.0.460",
    "@sprucelabs/jest-json-reporter": "^6.0.373",
    "@sprucelabs/jest-sheets-reporter": "^1.3.43",
    "@sprucelabs/mercury-client": "^22.2.90",
    "@sprucelabs/mercury-types": "^27.0.255",
    "@sprucelabs/resolve-path-aliases": "^1.1.37",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/test": "^7.7.311",
    "@sprucelabs/test-utils": "^3.0.538",
    "@types/inquirer": "^8.2.1",
    "@types/node": "17.0.5",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.1.0",
    "conventional-changelog-sprucelabs": "^1.1.2",
    "eslint": "^8.14.0",
    "eslint-config-spruce": "^10.11.2",
    "inquirer": "^8.2.4",
    "jest": "^28.0.3",
    "jest-circus": "^28.0.3",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "maxWorkers": 4,
    "testTimeout": 300000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/esm/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    },
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 843037832,
          "testMap": {
            "helpfulErrorIfVcIsBroken": "I155",
            "registersViewsOnBoot": "I162"
          }
        }
      ]
    ]
  }
}
